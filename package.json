{
    "name": "app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev:docker": "docker-compose -f docker-compose.dev.yml up -d",
        "dev:test": "dotenv -e .env.stagging.test.local next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint --fix && prettier . --write",
        "prepare": "husky install",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "prisma:explore": "prisma studio",
        "prisma:explore-test": "dotenv -e .env.stagging.test.local prisma studio",
        "postinstall": "prisma generate",
        "migrate": "prisma migrate dev"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@fontsource/roboto": "^5.0.8",
        "@mui/icons-material": "^5.14.12",
        "@mui/material": "^5.14.12",
        "@next-auth/prisma-adapter": "^1.0.7",
        "@prisma/client": "^5.5.2",
        "autoprefixer": "latest",
        "bcrypt": "^5.1.1",
        "dotenv": "^16.3.1",
        "formik": "^2.4.5",
        "mocha": "^10.2.0",
        "next": "^13.5.4",
        "next-auth": "^4.23.2",
        "pino": "^8.16.0",
        "react": "latest",
        "react-dom": "latest",
        "react-player": "^2.13.0",
        "react-toastify": "^9.1.3",
        "uuid": "^9.0.1",
        "yup": "^1.3.2"
    },
    "devDependencies": {
        "@testing-library/cypress": "^10.0.1",
        "@types/bcrypt": "^5.0.1",
        "@types/node": "latest",
        "@types/react": "latest",
        "@types/react-dom": "latest",
        "@types/uuid": "^9.0.6",
        "cypress": "^13.3.0",
        "dotenv-cli": "^7.3.0",
        "eslint": "latest",
        "eslint-config-next": "latest",
        "eslint-config-prettier": "^9.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "mochawesome": "^7.1.3",
        "mochawesome-json-to-md": "^0.7.2",
        "mochawesome-merge": "^4.3.0",
        "postcss": "latest",
        "prettier": "3.0.3",
        "prisma": "^5.5.2",
        "typescript": "latest"
    },
    "lint-staged": {
        "*.*": [
            "next lint",
            "prettier --write"
        ]
    }
}
