{
    "name": "app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "build:test": "dotenv -e .env.test.local next build",
        "start": "next start",
        "start:test": "dotenv -e .env.test.local next start",
        "lint": "next lint --fix && prettier . --write",
        "prepare": "husky install",
        "cypress:open": "dotenv -e .env.test.local cypress open",
        "prisma:explore": "prisma studio",
        "prisma:explore-test": "dotenv -e .env.test.local prisma studio"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@fontsource/roboto": "^5.0.8",
        "@mui/icons-material": "^5.14.12",
        "@mui/material": "^5.14.12",
        "@next-auth/prisma-adapter": "^1.0.7",
        "@prisma/client": "^5.4.0",
        "dotenv": "^16.3.1",
        "next": "^13.5.4",
        "next-auth": "^4.23.2",
        "react": "latest",
        "react-dom": "latest"
    },
    "devDependencies": {
        "@testing-library/cypress": "^10.0.1",
        "@types/node": "latest",
        "@types/react": "latest",
        "@types/react-dom": "latest",
        "autoprefixer": "latest",
        "cypress": "^13.3.0",
        "dotenv-cli": "^7.3.0",
        "eslint": "latest",
        "eslint-config-next": "latest",
        "eslint-config-prettier": "^9.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "postcss": "latest",
        "prettier": "3.0.3",
        "prisma": "^5.4.0",
        "typescript": "latest"
    },
    "lint-staged": {
        "*.*": [
            "next lint",
            "prettier --write"
        ]
    }
}
